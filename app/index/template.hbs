{{outlet}}

<div class="time-section">
{{#each model as |run|}}
{{#link-to "run-detail" run._id}}
  <div class="time-container">
    <div class="time-container__date">
      <h3 class="time-container__date-item">{{run.runDate}}</h3>
    </div>
    <div class="time-container__time">
      <h3 class="time-container__time-item">{{run.runTime}}</h3>
    </div>
  </div>
  {{/link-to}}
  {{/each}}
</div>

<footer class="footer">
{{#link-to "new"}}
  <button class="fa fa-plus add-new"></button>
{{/link-to}}
</footer>
